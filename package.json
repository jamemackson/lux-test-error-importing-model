{
  "name": "lux-test-error-importing-model",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "lux serve",
    "test": "NODE_ENV=test jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "knex": "0.12.6",
    "lux-framework": "1.1.7",
    "lux-jwt": "^0.2.2",
    "sqlite3": "3.1.8"
  },
  "engines": {
    "node": ">= 6.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.0",
    "jest": "^20.0.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": "\\.test\\.js$",
    "testEnvironment": "jest-environment-node",
    "cacheDirectory": "./.jest-cache",
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/_test/"
    ],
    "coveragePathIgnorePatterns": [
      "/db/",
      "/_test/",
      "/test/",
      "/__tests__/",
      "/node_modules/"
    ]
  }
}
